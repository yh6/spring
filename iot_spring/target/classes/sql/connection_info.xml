<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="connection">
  	<select id="selectConnectionInfo"  parameterType="ci" resultType="ci">
  		select ciName,ciUrl,ciPort,ciDatabase,ciUser,ciPwd,ciEtc from connection_info
   </select>
   
   
   <insert id="insertConnectionInfo"  parameterType="ci">
  		insert into connection_info(ciName,ciUrl,ciPort,ciDatabase,ciUser,ciPwd,ciEtc) 
  		values(#{ciName},#{ciUrl},#{ciPort},#{ciDatabase},#{ciUser},#{ciPwd},#{ciEtc})
   </insert>
 

  </mapper>